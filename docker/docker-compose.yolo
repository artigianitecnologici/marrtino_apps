  yolo:
    image: marrtino:yolo
    runtime: runc
    container_name: yolo
    dns: 8.8.8.8
    network_mode: "host"
    tty: true
    privileged: true
    environment:
    - MARRTINO_APPS_HOME=/home/robot/src/marrtino_apps
    volumes:
    - $MARRTINO_APPS_HOME:/home/robot/src/marrtino_apps
    - $HOME/playground:/home/robot/playground

